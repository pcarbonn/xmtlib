(set-option :backend none)
(declare-datatype T ( ( a ) ( b ) ))
(declare-fun ku (T T) Bool)
(x-interpret-fun ku
  (x-mapping
   ((a a) true)
   ((a b) false)
   ((b a) false)
  ) ?)
(declare-fun kuf (T) T)
(x-interpret-fun kuf (x-mapping
   ((a) a)
) ? )
(assert (forall ((x T)) (ku (kuf x) (kuf x)) ))
(x-ground :debug :sql)
(x-debug db _xmt_interp_ku_G)
(x-debug solver generators)
(x-debug db tables)
