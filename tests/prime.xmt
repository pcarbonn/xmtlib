(set-option :backend none)
(declare-fun domain (Int) Bool)
(declare-fun prime (Int) Bool)
(assert (forall ((x Int)) (=> (domain x)
                              (= (prime x)
                                 (forall ((y Int)) (=>  (and (domain y)
                                                             (< y x))
                                                        (> (mod x y) 0)))))))

(x-interpret-pred domain (x-range 2 10))
(x-ground)
(x-debug solver groundings)
